<h1>Appointment Index</h1>
<h3><em>List of all the appointments</em></h3> <p>

<ul>
<% @appointments.order(created_at: :desc).each do |appointment| %>
  <li> 
    <strong>Doctor Id:</strong> <%= appointment.doctor_id %><br />
    <strong>Patient Id:</strong> <%= appointment.patient_id %><br />
    <strong>Time:</strong> <%= appointment.time %><br /> 
    <strong>Room:</strong> <%= appointment.room %></br /><p>
  </li>
  <hr />
<% end %>
</ul>
